<!doctype html>
<html class="no-js">
   <head>
      <meta charset="utf-8" />
      <meta http-equiv="x-ua-compatible" content="ie=edge" />
      <title>{{title}}</title>
      <meta name="description" content="{{description}}" />
      <meta name="viewport" content="width=device-width, initial-scale=1" />
      <link rel="icon" href="/images/favicon.ico" />
      <meta name="google-site-verification" content="ismb3XGlL6apIUwFNBQSKLNEqz5BAPqEZFZWXy1uUGM" />

      <link href="https://fonts.googleapis.com/css?family=Graduate&amp;text=WebDeveloper" rel="stylesheet" />

      <!-- build:css(.tmp) /styles/ie.css -->
      <!--[if lt IE 9]>
      <link rel="stylesheet" href="/styles/no-mq.css" />
      <link rel="stylesheet" href="/styles/sprite.css" />
      <![endif]-->
      <!-- endbuild -->

      <!-- build:css(.tmp) /styles/main.css -->
      <!--[if (gte IE 9)|!(IE)]><!-->
      <!-- bower:css -->
      <!-- endbower -->
      <link rel="stylesheet" href="/styles/mq.css" />
      <link rel="stylesheet" href="/styles/sprite.css" />
      <!--<![endif]-->
      <!-- endbuild -->

      <!-- build:js /scripts/vendor/html5shiv.js -->
      <!--[if lt IE 9]>
      <script src="../bower_components/html5shiv/dist/html5shiv.js"></script>
      <script src="../bower_components/html5shiv/dist/html5shiv-printshiv.js"></script>
      <![endif]-->
      <!-- endbuild -->
   </head>
   <body {{#if controller}}data-controller="{{controller}}"{{/if}} {{#if action}}data-action="{{action}}" {{/if}}>
      {{> header}}

      {{> body}}

      {{> footer}}

      <script>
         window.ga=function(){ga.q.push(arguments)};ga.q=[];ga.l=+new Date;
         ga('create','UA-6265682-14','auto');ga('send','pageview')
      </script>
      <script src="https://www.google-analytics.com/analytics.js" async defer></script>

      <!-- build:js(.tmp) /scripts/main.js -->
      <!--[if (gte IE 9)|!(IE)]><!-->
      <!-- bower:js -->
      <script src="../bower_components/modernizr/modernizr.js"></script>
      <!-- endbower -->
      <script src="/scripts/main.js"></script>
      <!--<![endif]-->
      <!-- endbuild -->
   </body>
</html>
